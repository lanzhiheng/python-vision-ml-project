# Milvus 数据库配置
MILVUS_HOST=localhost
MILVUS_PORT=19530
MILVUS_USER=
MILVUS_PASSWORD=
MILVUS_DATABASE=default
MILVUS_COLLECTION_NAME=image_vectors

# CLIP 模型配置
CLIP_MODEL_NAME=ViT-B/32
CLIP_PRETRAINED=openai
CLIP_DEVICE=auto  # auto, cpu, cuda, mps

# 图片处理配置
IMAGE_SIZE=224
BATCH_SIZE=32
MAX_IMAGE_SIZE_MB=10

# 向量配置
VECTOR_DIMENSION=512
INDEX_TYPE=IVF_FLAT
METRIC_TYPE=L2
NLIST=1024

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=logs/image_vectorization.log

# 缓存配置
ENABLE_CACHE=true
CACHE_SIZE=1000
CACHE_TTL=3600